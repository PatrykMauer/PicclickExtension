{
  "name": "Picclick extension",
  "description": "Shows newly listed items in specified range of time",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "./icons/16x16.png",
    "32": "./icons/32x32.png",
    "48": "./icons/48x48.png",
    "128": "./icons/128x128.png"
  },
  "background": {
    "scripts": ["./background.js"]
  },
  "options_page": "./options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://picclick.com/",
    "https://picclick.de/",
    "https://picclick.com.au/",
    "https://picclick.at/",
    "https://picclick.be/",
    "https://fr.picclick.be/",
    "https://picclick.ca/",
    "https://picclick.fr/",
    "https://picclick.it/",
    "https://picclick.ie/",
    "https://picclick.nl/",
    "https://picclick.es/",
    "https://picclick.ch/",
    "https://picclick.co.uk/",
    "storage"
  ]
}
