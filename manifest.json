{
  "name": "Picclick extension",
  "description": "Shows newly listed items in specified range of time",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "./icons/16x16.png",
    "32": "./icons/32x32.png",
    "48": "./icons/48x48.png",
    "128": "./icons/128x128.png"
  },
  "background": {
    "scripts": ["./background.js"]
  },
  "options_page": "./options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "https://picclick.com/", "storage"]
}
